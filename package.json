{
  "name": "ct-blog",
  "version": "1.0.0",
  "private": true,
  "description": "/ct-blog",
  "scripts": {
    "develop": "./node_modules/.bin/gatsby develop",
    "start": "NODE_ENV=development npm run clean && npm run develop",
    "build": "./node_modules/.bin/gatsby build --prefix-paths",
    "serve": "./node_modules/.bin/gatsby serve",
    "clean": "./node_modules/.bin/gatsby clean",
    "deploy": "NODE_ENV=production npm run clean && npm run build && gh-pages -d public",
    "build-test": "NODE_ENV=production npm run clean && npm run build"
  },
  "dependencies": {
    "@deckdeckgo/highlight-code": "2.6.0",
    "@styled-icons/fa-solid": "10.32.0",
    "algoliasearch": "4.9.1",
    "antd": "4.15.2",
    "dotenv": "8.2.0",
    "gatsby": "3.2.1",
    "gatsby-plugin-algolia": "0.19.0",
    "gatsby-plugin-baidu-analytics": "2.2.0",
    "gatsby-plugin-google-analytics": "^3.4.0",
    "gatsby-plugin-google-analytics-reporter": "^1.1.1",
    "gatsby-plugin-google-gtag": "3.3.0",
    "gatsby-plugin-less": "5.3.0",
    "gatsby-plugin-postcss": "4.3.0",
    "gatsby-plugin-react-helmet": "4.3.0",
    "gatsby-plugin-sharp": "3.3.1",
    "gatsby-plugin-styled-components": "4.3.0",
    "gatsby-plugin-use-query-params": "1.0.1",
    "gatsby-remark-autolink-headers": "4.0.0",
    "gatsby-remark-copy-linked-files": "4.0.0",
    "gatsby-remark-highlight-code": "2.2.0",
    "gatsby-remark-images": "5.0.0",
    "gatsby-remark-prismjs": "5.0.0",
    "gatsby-remark-relative-images": "2.0.2",
    "gatsby-remark-responsive-iframe": "4.0.0",
    "gatsby-remark-smartypants": "4.0.0",
    "gatsby-source-filesystem": "3.3.0",
    "gatsby-source-google-analytics-reporting-api": "^1.0.2",
    "gatsby-transformer-remark": "4.0.0",
    "gatsby-transformer-sharp": "3.3.0",
    "moment": "2.29.1",
    "prismjs": "1.23.0",
    "prop-types": "15.7.2",
    "query-string": "7.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-instantsearch-dom": "6.10.3",
    "styled-components": "5.2.3",
    "use-query-params": "1.2.2"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint-config-react-app": "6.0.0",
    "gh-pages": "3.1.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
