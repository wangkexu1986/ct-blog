{
  "name": "ct-blog",
  "version": "1.0.0",
  "private": true,
  "description": "ct-blog",
  "scripts": {
    "start": "./node_modules/.bin/gatsby develop",
    "build": "./node_modules/.bin/gatsby build",
    "serve": "./node_modules/.bin/gatsby serve",
    "clean": "./node_modules/.bin/gatsby clean",
    "deploy": "npm run build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "antd": "^4.15.2",
    "gatsby": "^3.2.1",
    "gatsby-plugin-baidu-analytics": "^2.2.0",
    "gatsby-plugin-google-gtag": "^3.3.0",
    "gatsby-plugin-postcss": "^4.3.0",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sharp": "^3.3.0",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-remark": "^4.0.0",
    "gatsby-transformer-sharp": "^3.3.0",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint-config-react-app": "^6.0.0",
    "gh-pages": "^3.1.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
